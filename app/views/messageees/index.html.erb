<h1>Listing messageees</h1>

<table>
  <thead>
    <tr>
      <th>Asd</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @messageees.each do |messageee| %>
      <tr>
        <td><%= messageee.asd %></td>
        <%= image_tag messageee.image_url(:thumb) if messageee.image? %>
        <td><%= link_to 'Show', messageee %></td>
        <td><%= link_to 'Edit', edit_messageee_path(messageee) %></td>
        <td><%= link_to 'Destroy', messageee, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Messageee', new_messageee_path %>
